    "async": "^3.2.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.4.20",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.16",
    "qrcode.react": "^1.0.0",
    "react": "^16.13.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.5",
    "saslprep": "^1.0.3",
    "sharp": "^0.25.4",
